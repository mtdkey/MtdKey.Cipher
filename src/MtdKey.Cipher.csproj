<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Description>The library for exchanging encrypted messages between different applications.
Allows messages to be exchanged kind of class objects.</Description>
    <Copyright>https://github.com/mtdkey/MtdKey.Cipher</Copyright>
    <PackageProjectUrl>https://github.com/mtdkey/mtdkey.cipher</PackageProjectUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <RepositoryUrl>https://github.com/mtdkey/mtdkey.cipher</RepositoryUrl>
    <Title>MTD Key Cipher</Title>
    <PackageTags>mtdkey, dotnet, aes, encryptor, Rijndael</PackageTags>
    <Version>1.1.1</Version>
    <Authors>Oleg Bruev</Authors>
    <Company>MTD Key</Company>
    <AssemblyVersion>1.1.1</AssemblyVersion>
    <FileVersion>1.1.1</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>
	
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Options" Version="8.0.2" />
    <PackageReference Include="Microsoft.IdentityModel.Tokens" Version="7.6.0" />
  </ItemGroup>

</Project>
